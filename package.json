  "license": "MIT",
  "devDependencies": {
    "tailwindcss": "^3.3.0",
    "live-server": "^1.2.2",
    "autoprefixer": "^10.4.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0"
  }
}
