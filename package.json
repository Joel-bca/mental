{
  "name": "mental-health-tracker",
  "version": "1.0.0",
  "description": "A mental health tracking app for children and youth with mood tracking and AI suggestions",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000",
    "build": "tailwindcss -i ./src/css/input.css -o ./dist/css/output.css --watch",
